				{
					"name": "POST login user",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"(() => {\r",
									"  try {\r",
									"    const j = pm.response.json();\r",
									"\r",
									"    // Guardar Token JWT principal (token)\r",
									"    if (j && j.token) {\r",
									"      pm.environment.set(\"token\", j.token);\r",
									"      console.log(\"Token guardado:\", j.token);\r",
									"    }\r",
									"\r",
									"    // Guardar Refresh Token\r",
									"    if (j && j.refreshToken) {\r",
									"      pm.environment.set(\"refreshToken\", j.refreshToken);\r",
									"      console.log(\"Refresh token guardado:\", j.refreshToken);\r",
									"    }\r",
									"\r",
									"    // Guardar ID do utilizador\r",
									"    if (j.user?.id) {\r",
									"      pm.environment.set(\"userId\", j.user.id);\r",
									"      console.log(\"User ID guardado:\", j.user.id);\r",
									"    }\r",
									"\r",
									"    // Guardar nickname do utilizador\r",
									"    if (j.user?.nickName) {\r",
									"      pm.environment.set(\"nickName\", j.user.nickName);\r",
									"      console.log(\"Nickname guardado:\", j.user.nickName);\r",
									"    }\r",
									"\r",
									"    //guardar email\r",
									"    if (j.user?.email) {\r",
									"    pm.environment.set(\"email\", j.user.email);\r",
									"     console.log(\"Email guardado:\", j.user.email);\r",
									"    }\r",
									"\r",
									"\r",
									"  } catch (e) {\r",
									"    console.error(\"Erro ao processar resposta JSON:\", e);\r",
									"  }\r",
									"})();\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
